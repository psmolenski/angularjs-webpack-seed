{
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(html)$": "<rootDir>/__mocks__/htmlMock.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.ts"
    }
  }